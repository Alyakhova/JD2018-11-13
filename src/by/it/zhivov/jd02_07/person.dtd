<?xml version="1.0" encoding="UTF-8"?>
        <!ELEMENT persons (user+,admin,guest?)>
        <!ELEMENT user (name,dateOfBirth,email,tel,privelegy,ads)>
        <!ATTLIST user
                login ID #REQUIRED
                password CDATA #REQUIRED>
        <!ATTLIST admin
                login ID #REQUIRED
                password CDATA #REQUIRED>
        <!ELEMENT name (#PCDATA)>
        <!ELEMENT dateOfBirth (#PCDATA)>
        <!ELEMENT email (#PCDATA)>
        <!ELEMENT tel (#PCDATA)>
        <!ELEMENT privelegy (#PCDATA)>
        <!ELEMENT ads (ad)*>
        <!ELEMENT ad (description,brand,model,color,body,year,equipment,mileage,crashed,price)>
        <!ELEMENT description (#PCDATA)>
        <!ELEMENT brand (#PCDATA)>
        <!ELEMENT model (#PCDATA)>
        <!ELEMENT color (#PCDATA)>
        <!ELEMENT body (#PCDATA)>
        <!ELEMENT year (#PCDATA)>
        <!ELEMENT equipment (#PCDATA)>
        <!ELEMENT mileage (#PCDATA)>
        <!ELEMENT crashed (#PCDATA)>
        <!ELEMENT price (#PCDATA)>
        <!ELEMENT admin (privelegy)>
        <!ELEMENT guest (privelegy)>



